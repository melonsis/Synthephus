[tool.uv]
cache-dir = "path-to-uv-cache"

[[tool.uv.index]]
url = "https://mirrors.tuna.tsinghua.edu.cn/pypi/web/simple"
default = true


[project]
name = "dolphin_proto"
version = "0.1.0"
description = "codebase of dolphin prototype"
readme = "README.md"
requires-python = "==3.12.0"
dependencies = [
    "attrs",
    "numpy",
    "scipy==1.13.0",
    "pandas",
    "networkx",
    "matplotlib",
    "disjoint-set",
    "autodp @ file:autodp-0.1.1.tar",
    "serverchan_sdk",
    "tqdm",
    "psutil"
]
